<facelet-taglib version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
        http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd">

    <namespace>http://rekall.cent4ur.hu</namespace>

    <tag>
        <tag-name>booleanValue</tag-name>
        <source>tag/booleanValue.xhtml</source>
        <attribute>
            <description>Component identifier.</description>
            <name>id</name>
        </attribute>
        <attribute>
            <description>The boolean value that should be rendered.</description>
            <name>value</name>
        </attribute>
    </tag>

</facelet-taglib>